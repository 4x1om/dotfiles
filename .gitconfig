[user]
	email = birduedev@gmail.com
	name = birdue
[core]
	editor = vim
[init]
	defaultBranch = main
[alias]
	s = status
	a = add
	b = branch
	c = commit
	d = diff
	l = log
	sw = switch
	ba = branch --all
	aa = add --all
	nb = switch -c
	rb = branch -d
	g = log --all --oneline --graph
	co = checkout
	cm = commit -m
	ds = diff --staged
	sub = restore --staged
[credential]
	helper = cache --timeout 43200
